{
  "name": "monarch-app",
  "version": "3.0.0",
  "description": "Monarch web application and API",
  "scripts": {
    "clean": "npm run wbs-clean-dist",
    "dev": "npm run wbs-dev",
    "devbeta": "USE_LOG_FETCH=0 USE_WEBPACK=1 NODE_PATH=./lib/monarch node_modules/.bin/nodemon -d 2 -V -- lib/monarch/web/webapp_launcher.js beta & LINTER=0 node_modules/.bin/webpack-dev-server --config ./webpack.config.js --port 8081 --history-api-fallback --hot --inline",
    "build": "npm run wbs-build",
    "oldclean": "rm -rf ./dist/*",
    "olddev": "USE_LOG_FETCH=0 USE_WEBPACK=1 NODE_PATH=./lib/monarch node_modules/.bin/nodemon -d 2 -V -- lib/monarch/web/webapp_launcher.js & node_modules/.bin/webpack-dev-server --config ./webpack.make.js --port 8081 --history-api-fallback --hot --inline",
    "olddevbeta": "USE_LOG_FETCH=0 USE_WEBPACK=1 NODE_PATH=./lib/monarch node_modules/.bin/nodemon -d 2 -V -- lib/monarch/web/webapp_launcher.js beta & node_modules/.bin/webpack-dev-server --config ./webpack.make.js --port 8081 --history-api-fallback --hot --inline",
    "oldbuild": "npm run oldclean && BUILD=1 ./node_modules/webpack/bin/webpack.js --config webpack.make.js --bail -p",
    "oldstart": "NODE_PATH=./lib/monarch node lib/monarch/web/webapp_launcher.js",
    "start": "NODE_PATH=./lib/monarch node lib/monarch/web/webapp_launcher.js",
    "startbeta": "NODE_PATH=./lib/monarch node lib/monarch/web/webapp_launcher.js beta",
    "wbs-dev": "USE_LOG_FETCH=0 USE_WEBPACK=1 NODE_PATH=./lib/monarch node_modules/.bin/nodemon -d 2 -V -- lib/monarch/web/webapp_launcher.js & LINTER=0 MODE_DEV_SERVER=1 node_modules/.bin/webpack-dev-server --config ./webpack.config.js --port 8081 --history-api-fallback --hot --inline",
    "wbs-analyze": "npm run wbs-clean-dist && ANALYZE=1 node_modules/.bin/webpack --bail --progress --colors",
    "wbs-postinstall": "node ./wpcontributing/bin/install-warning-message.js",
    "wbs-start": "LINTER=0 npm run wbs-webpack",
    "wbs-watch": "npm run wbs-clean-dist && LINTER=0 ./node_modules/.bin/webpack --progress --watch",
    "wbs-test": "npm run wbs-lint",
    "wbs-lint": "./node_modules/.bin/eslint -c wbs.eslintrc wpsrc",
    "wbs-lint-fix": "./node_modules/.bin/eslint --fix wpsrc --ext .js",
    "wbs-clean-dist": "node ./wpbin/clean-dist.js",
    "wbs-serve-build": "echo 'Serving distribution folder dist/' && npm run wbs-serve-dist",
    "wbs-serve-dist": "./node_modules/.bin/serve -p 8081 dist/",
    "wbs-build": "npm run wbs-clean-dist && npm run wbs-webpack-build",
    "wbs-build-prod": "npm run wbs-clean-dist && NODE_ENV=production npm run wbs-webpack-build-prod",
    "wbs-build-prod-all": "DEVTOOLS=false npm run wbs-build-prod && NODE_ENV=production OPTIMIZE=false DEVTOOLS=true DIST_DIR=dist/devtools npm run wbs-webpack-build",
    "wbs-postbuild-prod-all": "npm run wbs-generate-dist-readme",
    "wbs-webpack": "./node_modules/.bin/webpack-dev-server --progress --colors --hot --inline",
    "wbs-webpack-build": "LINTER=0 ./node_modules/.bin/webpack --progress",
    "wbs-webpack-build-prod": "LINTER=0 ./node_modules/.bin/webpack --progress -p",
    "wbs-generate-dist-readme": "node wpbin/generate-dist-readme.js",
    "wbs-contributing-install": "./wpcontributing/bin/contributing-install.sh",
    "wbs-contributing-test": "./wpcontributing/bin/contributing-test.sh"
  },
  "keywords": [
    "Monarch Initiative"
  ],
  "author": "Monarch Initiative <info@monarchinitiative.org>",
  "homepage": "http://monarchinitiative.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/monarch-initiative/monarch-app"
  },
  "engines": {
    "node": ">= 6.11.2",
    "npm": ">= 5.3.0"
  },
  "bugs": {
    "url": "https://github.com/monarch-initiative/monarch-app/issues"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync-webpack-plugin": "^1.2.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "eslint": "^4.7.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "7",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "3",
    "file-loader": "^0.11.2",
    "git-rev-sync": "^1.9.1",
    "gulp": "^3.9.1",
    "gulp-download": "0.0.1",
    "gulp-jsoncombine": "^1.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.6.3",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "is-my-json-valid": "^2.16.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash.template": "^4.4.0",
    "mocha": "^3.5.3",
    "moment": "^2.18.1",
    "my-local-ip": "^1.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npmlog": "^4.1.2",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "serve": "^6.1.0",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.1.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.8.2"
  },
  "dependencies": {
    "async": "^2.5.0",
    "bbop": "^2.4.3",
    "bbop-graph": "0.0.19",
    "bbop-manager-golr": "0.0.12",
    "bbop-registry": "0.0.3",
    "bbop-response-golr": "0.0.8",
    "bbop-rest-manager": "0.0.17",
    "bbop-widget-set": "^2.5.8",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0",
    "golr-conf": "0.0.3",
    "hapi": "^16.6.0",
    "inert": "^4.2.1",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "mustache": "^2.3.0",
    "navigo": "^5.3.1",
    "phenogrid": "^1.3.11",
    "pup-tent": "^0.9.10",
    "request": "^2.82.0",
    "underscore": "^1.8.3",
    "wait.for": "^0.6.6",
    "xml2js": "^0.4.19"
  }
}
