<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta
    name="viewport"
    content="initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
  <title>
    Welcome to <%= htmlWebpackPlugin.options.title %>
  </title>
  <% if (!htmlWebpackPlugin.options.MODE_DEV_SERVER) { %>
    <link
      href="<%= webpackConfig.output.publicPath + (Object.prototype.toString.call(webpack.assetsByChunkName['app']) === '[object Array]' ? webpack.assetsByChunkName['app'][1] : webpack.assetsByChunkName['app']) %>"
      rel="stylesheet">
    <% } %>

    <style>
    body.monarch-cloak {
      display: none;
    }
    </style>

    <!-- Legacy JS launcher. -->
    <script type="text/javascript">


function loaderBody() {
  var bodyDOM = document.getElementsByTagName('body')[0];
  bodyDOM.classList.remove('monarch-cloak');
  InitMonarchBBOPHandler();
  InitMonarchBBOPLinker();
  InitMonarchBBOPWidgetBrowse();
  InitMonarchBBOPWidgetDisplay();
  // InitMonarchPage();
  // InitTabs();
  // InitFacetFilters();
}
    </script>

  <% if (htmlWebpackPlugin.options.MODE_LOCAL_SERVER) { %>
    <script>
      window.mngLocalServerMode = true;
    </script>
  <% } %>

</head>

<body
  id="monarch-body"
  class="monarch-cloak"
  onload="loaderBody()">

  <div
    id="app">
  </div>

  <div class="dynamic-import-test">
    <p class="require-ensure">
      <span class="test-require-ensure">
        <img
          src="<%= require('./assets/img/monarch-logo.png') %>"
          class="fidget-spinner left"
          alt="monarch-logo"/>
        <img
          src="<%= require('./assets/img/monarch-logo.png') %>"
          class="fidget-spinner right"
          alt="monarch-logo"/>
      </span>
    </p>
  </div>

  <!-- keep the following lines -->
  <% if (!htmlWebpackPlugin.options.MODE_DEV_SERVER) { %>
    <script
      src="<%= webpackConfig.output.publicPath + (Object.prototype.toString.call(webpack.assetsByChunkName['app']) === '[object Array]' ? webpack.assetsByChunkName['app'][0] : webpack.assetsByChunkName['app']) %>"></script>
  <% } %>

